library(tidyverse)
# Load dataset
nba <- read_csv("nba_2017_br.csv")
# Select and clean relevant variables
nba_clean <- nba %>%
select(Player, Pos, MP, `PS/G`) %>%
filter(!is.na(MP), !is.na(`PS/G`)) %>%
rename(PSG = `PS/G`)
scatter_plot <- nba_clean %>%
ggplot(aes(x = MP, y = PSG, colour = Pos)) +
geom_point(alpha = 0.7) +
geom_smooth(method = "lm", se = TRUE, colour = "black") +
labs(
title = "Scatter Plot of Minutes per Game vs Points per Game",
x = "Minutes per Game (MP)",
y = "Points per Game (PS/G)",
colour = "Player Position"
) +
theme_minimal()
print(scatter_plot)
hist_plot <- nba_clean %>%
ggplot(aes(x = MP)) +
geom_histogram(binwidth = 2, fill = "lightblue", color = "black") +
labs(
title = "Histogram of Minutes per Game",
x = "Minutes per Game (MP)",
y = "Count of Players"
) +
theme_minimal()
print(hist_plot)
# Pearson correlation
pearson_result <- cor.test(nba_clean$MP, nba_clean$PSG, method = "pearson")
print("Pearson Correlation Result:")
print(pearson_result)
library(tidyverse)
# Load dataset
nba <- read_csv("nba_2017_br.csv")
# Clean/select variables
nba_clean <- nba %>%
select(Player, Pos, MP, `PS/G`) %>%
filter(!is.na(MP), !is.na(`PS/G`)) %>%
rename(PSG = `PS/G`)
scatter_plot <- nba_clean %>%
ggplot(aes(x = PSG, y = MP, colour = Pos)) +
geom_point(alpha = 0.7) +
geom_smooth(method = "lm", se = TRUE, colour = "black") +
labs(
title = "Scatter Plot of Points per Game vs Minutes per Game",
x = "Points per Game (PS/G)",
y = "Minutes per Game (MP)",
colour = "Position"
) +
theme_minimal()
print(scatter_plot)
# Compute mean & sd for bell curve
mp_mean <- mean(nba_clean$MP)
mp_sd   <- sd(nba_clean$MP)
hist_bell <- nba_clean %>%
ggplot(aes(x = MP)) +
geom_histogram(
aes(y = ..density..),
binwidth = 2,
fill = "lightblue",
colour = "black"
) +
stat_function(
fun = dnorm,
args = list(mean = mp_mean, sd = mp_sd),
colour = "red",
linewidth = 1
) +
labs(
title = "Histogram of Minutes per Game with Bell Curve Overlay",
x = "Minutes per Game (MP)",
y = "Density"
) +
theme_minimal()
print(hist_bell)
spearman_result <- cor.test(nba_clean$PSG, nba_clean$MP, method = "spearman")
